[user]
    name  = Jeremy Warner
    email = jeremywrnr@gmail.com
[credential]
    helper = osxkeychain
[push]
    default = current
[pull]
    rebase = true
[rebase]
    autoStash = true
[fetch]
    prune = true
[init]
    defaultBranch = main
[alias]
    s= status
    as= all-status
    a= add -A
    c= commit -am
    d= diff
    t= tag -n9
    wc=log --numstat --oneline
    lp= log -p
    lg= log --oneline --decorate --color --graph
    lgg=log --pretty=format:'%C(yellow)%h %C(cyan)%ad%Cgreen%d %Creset%s' --date=short
    la= log --graph --decorate --pretty=format:'%C(yellow)%h %Cred%cr %Cblue(%an)%C(white)%d%Creset %s' --abbrev-commit --all
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    lb= link-branches
    br= branch
    co= checkout
    ps= push
    pl= pull
    open = !open-remote
    last= log -p --max-count=1 --word-diff
    alias= config --get-regexp alias
    up = pull --rebase --autostash
    ac= add-commit
    acp= add-commit-push
    puller= all-puller
    set-upstream = \
        !git branch \
        --set-upstream-to=origin/`git symbolic-ref --short HEAD`
[diff]
    algorithm = minimal
    compactionHeuristic = true
    renames = true
[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
    ui = true
[color "diff"]
    old = red bold
    new = green bold
    frag = magenta bold
    whitespace = red reverse
[color "diff-highlight"]
    oldNormal = red bold
    oldHighlight = "red bold 52"
    newNormal = "green bold"
    newHighlight = "green bold 22"
